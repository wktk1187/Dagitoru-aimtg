{
  "version": 2,
  "builds": [
    {
      "src": "apps/aimtg/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/aimtg/$1"
    }
  ],
  "functions": {
    "api/upload-url/route.ts": {
      "runtime": "node"
    },
    "api/slack/events/route.ts": {
      "runtime": "edge"
    },
    "api/notion-sync/route.ts": {
      "runtime": "edge"
    }
  }
}